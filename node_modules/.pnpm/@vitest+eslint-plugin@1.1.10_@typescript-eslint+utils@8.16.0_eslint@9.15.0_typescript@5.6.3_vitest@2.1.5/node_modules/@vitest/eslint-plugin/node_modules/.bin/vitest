#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/leslie/Documents/projects/vue-playground/node_modules/.pnpm/vitest@2.1.5_@types+node@22.9.1_jsdom@25.0.1/node_modules/vitest/node_modules:/Users/leslie/Documents/projects/vue-playground/node_modules/.pnpm/vitest@2.1.5_@types+node@22.9.1_jsdom@25.0.1/node_modules:/Users/leslie/Documents/projects/vue-playground/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/leslie/Documents/projects/vue-playground/node_modules/.pnpm/vitest@2.1.5_@types+node@22.9.1_jsdom@25.0.1/node_modules/vitest/node_modules:/Users/leslie/Documents/projects/vue-playground/node_modules/.pnpm/vitest@2.1.5_@types+node@22.9.1_jsdom@25.0.1/node_modules:/Users/leslie/Documents/projects/vue-playground/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vitest@2.1.5_@types+node@22.9.1_jsdom@25.0.1/node_modules/vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../../../../../../vitest@2.1.5_@types+node@22.9.1_jsdom@25.0.1/node_modules/vitest/vitest.mjs" "$@"
fi
